<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.junshijun.hub.idea.mapper.SysApiMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.junshijun.hub.idea.entity.SysApi">
        <id column="api_id" property="apiId"/>
        <result column="api_code" property="apiCode"/>
        <result column="api_name" property="apiName"/>
        <result column="api_comments" property="apiComments"/>
        <result column="api_url" property="apiUrl"/>
        <result column="api_method_type" property="apiMethodType"/>
        <result column="version" property="version"/>
        <result column="is_deleted" property="isDeleted"/>
        <result column="created_by" property="createdBy"/>
        <result column="created_time" property="createdTime"/>
        <result column="modified_by" property="modifiedBy"/>
        <result column="modified_time" property="modifiedTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        api_id
        , api_code, api_name, api_comments, version, is_deleted, created_by, created_time, modified_by, modified_time
    </sql>

</mapper>
